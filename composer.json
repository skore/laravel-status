{
    "name": "skore-labs/laravel-status",
    "description": "Laravel code-typed statuses for models",
    "license": "MIT",
    "keywords": [
        "skore-labs",
        "laravel",
        "statuses",
        "eloquent",
        "typed"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "spatie/enum": "^3.9",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "laravel/nova": "^2.0|^3.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "suggest": {
        "laravel/nova": "Required if using status nova filter"
    },
    "autoload": {
        "psr-4": {
            "SkoreLabs\\LaravelStatus\\": "src",
            "SkoreLabs\\LaravelStatus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkoreLabs\\LaravelStatus\\ServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
